{
  "name": "prosfin-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "start": "turbo start",
    "lint": "turbo lint",
    "dev:web": "pnpm --filter prosfin-web dev",
    "dev:admin": "pnpm --filter prosfin-admin dev",
    "build:web": "pnpm --filter prosfin-web build",
    "build:admin": "pnpm --filter @prosfin/db db:generate && pnpm --filter prosfin-admin build",
    "db:generate": "pnpm --filter @prosfin/db db:generate"
  },
  "devDependencies": {
    "typescript": "^5",
    "turbo": "^2.3.3"
  },
  "dependencies": {
    "@react-three/drei": "^10.7.7",
    "@react-three/fiber": "^9.4.2",
    "@react-three/postprocessing": "^3.0.4",
    "postprocessing": "^6.38.1",
    "three": "^0.182.0"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  }
}
